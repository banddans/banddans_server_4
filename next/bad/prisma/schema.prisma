generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Beach {
  id           String        @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  temperatures Temperature[]
}

model Temperature {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  value   Float
  time    DateTime
  beachId String   @db.ObjectId
  beach   Beach    @relation(fields: [beachId], references: [id])
}
